    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "recursive-copy": "^2.0.6",
    "remark": "^7.0.1",
    "remark-autolink-headings": "^5.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^6.0.0",
    "remark-slug": "^4.2.2"
  },
  "devDependencies": {
    "fly": "^2.0.6",
    "fly-babel": "^2.1.1",
    "fly-clear": "^1.0.1",
    "fly-esnext": "^2.0.1",
