  "scripts": {
    "build": "fly",
    "release": "fly release",
    "serve": "serve out -s",
    "docs": "./dist/bin/cli.js docs",
    "pages": "npm run docs && npm run fix:link && npm run fix:static && gh-pages -d out",
    "fix:link": "replace '<a href=\"/' '<a href=\"/next-book/' ./out -r --include='*.html'",
    "fix:static": "replace '/static/' '/next-book/static/' ./out -r --include='*.html'"
  },
